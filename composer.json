{
    "name": "dmp/cqrs-sf-bundle",
    "description": "CQRS Basic symfony bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "symfony/http-kernel": "^5.4 || ^6.2",
        "symfony/messenger": "^5.4 || ^6.2"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpunit/phpunit": "^9.5 || ^10.1"
    },
    "autoload": {
        "psr-4": {
            "DMP\\CQRS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DMP\\CQRS\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-cc": "phpunit coverage-html report/"
    }
}
